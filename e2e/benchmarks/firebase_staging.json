{
  "hosting": {
    "site": "tfjs-benchmarks-staging",
    "public": ".",
    "ignore": [
      "firebase_prod.json",
      "firebase_staging.json"
    ],
    // Set up headers for cross-origin isolation.
    "headers": [{
      "source": "local-benchmark/",
      "headers": [
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        }
      ]
    }]
  }
}
